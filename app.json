{
  "expo": {
    "name": "FLOPPAS-COOK",
    "slug": "FLOPPAS-COOK",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/img/LOGO-FLOPPAS-GREEN.png",
      "resizeMode": "contain",
      "backgroundColor": "#0c0f12"
